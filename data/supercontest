server {
    server_name southbaysupercontest.com www.southbaysupercontest.com;

    location / {
        include uwsgi_params;
        uwsgi_pass unix:/home/bmahlstedt/code/supercontest/data/supercontest.sock;
    }

    listen 443 ssl; # managed by Certbot
    ssl_certificate /etc/letsencrypt/live/southbaysupercontest.com/fullchain.pem; # managed by Certbot
    ssl_certificate_key /etc/letsencrypt/live/southbaysupercontest.com/privkey.pem; # managed by Certbot
    include /etc/letsencrypt/options-ssl-nginx.conf; # managed by Certbot
    ssl_dhparam /etc/letsencrypt/ssl-dhparams.pem; # managed by Certbot


}
server {
    if ($host = www.southbaysupercontest.com) {
        return 301 https://$host$request_uri;
    } # managed by Certbot


    if ($host = southbaysupercontest.com) {
        return 301 https://$host$request_uri;
    } # managed by Certbot


    listen 80;
    server_name southbaysupercontest.com www.southbaysupercontest.com;
    return 404; # managed by Certbot




}
